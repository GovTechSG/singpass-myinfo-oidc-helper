		"is-base64": "^1.1.0",
		"lodash": "^4.17.21",
		"node-jose": "^2.1.1",
		"nonce": "^1.0.4",
		"proxy-agent": "^5.0.0",
		"rosie": "^2.1.0"
	},
	"devDependencies": {
		"@types/jest": "^27.4.1",
		"@types/lodash": "^4.14.181",
		"@types/node": "^17.0.23",
		"@types/rosie": "0.0.40",
		"dotenv": "^16.0.0",
		"dtsgenerator": "^3.15.1",
		"handlebars": "^4.7.7",
		"husky": "^7.0.4",
		"jest": "^27.5.1",
		"jest-bamboo-formatter": "^1.0.1",
		"lint-staged": "^12.3.7",
		"nock": "^13.2.4",
		"shelljs": "~0.8.5",
		"ts-jest": "^27.1.4",
		"ts-node": "^10.7.0",
		"tslint": "^6.1.3",
		"tslint-config-security": "^1.16.0",
		"tslint-no-circular-imports": "~0.7.0",
